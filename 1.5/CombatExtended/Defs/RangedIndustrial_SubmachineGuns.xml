<Defs>
  <!--=SUBMACHINE GUNS=-->
  <!--PDW2000-->
  <ThingDef ParentName="BaseHumanMakeableGun">
    <defName>UACGGun_PDW2k</defName>
    <label>PDW2000</label>
    <description>The PDW2000 is a widespread submachine gun favored for its size and low recoil. The weapon has a conventional submachine gun layout with the magazine housed in the pistol grip and mirrored controls to achieve a truly ambidextrous use. Its low weight and small profile can be compared with some larger pistols. Two rails for mounting sights and tactical accessories underline the weapon's utility.</description>
    <graphicData>
      <texPath>Things/Weapons/HIDGun_PDW2000</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <drawSize>(1.08,1.08)</drawSize>
    </graphicData>
    <soundInteract>Interact_SMG</soundInteract>
    <recipeMaker>
      <researchPrerequisite>BlowbackOperation</researchPrerequisite>
      <skillRequirements>
        <Crafting>5</Crafting>
      </skillRequirements>
    </recipeMaker>
    <thingSetMakerTags>
      <li>RewardStandardQualitySuper</li>
    </thingSetMakerTags>
    <statBases>
      <Mass>1.50</Mass>
      <RangedWeapon_Cooldown>0.37</RangedWeapon_Cooldown>
      <SightsEfficiency>1.00</SightsEfficiency>
      <ShotSpread>0.15</ShotSpread>
      <SwayFactor>0.65</SwayFactor>
      <Bulk>3.50</Bulk>
      <WorkToMake>28500</WorkToMake>
    </statBases>
    <weaponTags>
      <li>IndustrialGunAdvanced</li>
      <li>ShortShots</li>
      <li>CE_SMG</li>
      <li>CE_AI_BROOM</li>
      <li>CE_OneHandedWeapon</li>
      <li>A3PDW2k</li>
    </weaponTags>
    <weaponClasses>
      <li>ShortShots</li>
      <li>RangedLight</li>
    </weaponClasses>
    <costList>
      <Steel>25</Steel>
      <Chemfuel>5</Chemfuel>
      <ComponentIndustrial>5</ComponentIndustrial>
    </costList>
    <verbs>
      <li Class="CombatExtended.VerbPropertiesCE">
        <recoilAmount>1.89</recoilAmount>
        <verbClass>CombatExtended.Verb_ShootCE</verbClass>
        <hasStandardCommand>true</hasStandardCommand>
        <defaultProjectile>Bullet_9x19mmPara_FMJ</defaultProjectile>
        <burstShotCount>6</burstShotCount>
        <ticksBetweenBurstShots>4</ticksBetweenBurstShots>
        <warmupTime>0.6</warmupTime>
        <range>20</range>
        <soundCast>HID_Shot_A3PDW2k</soundCast>
        <soundCastTail>GunTail_Light</soundCastTail>
        <muzzleFlashScale>9</muzzleFlashScale>
      </li>
    </verbs>
    <tools>
      <li Class="CombatExtended.ToolCE">
        <label>stock</label>
        <capacities>
          <li>Blunt</li>
        </capacities>
        <power>8</power>
        <cooldownTime>1.55</cooldownTime>
        <chanceFactor>1.5</chanceFactor>
        <armorPenetrationBlunt>2.755</armorPenetrationBlunt>
        <linkedBodyPartsGroup>Stock</linkedBodyPartsGroup>
      </li>
      <li Class="CombatExtended.ToolCE">
        <label>barrel</label>
        <capacities>
          <li>Blunt</li>
        </capacities>
        <power>5</power>
        <cooldownTime>2.02</cooldownTime>
        <armorPenetrationBlunt>1.630</armorPenetrationBlunt>
        <linkedBodyPartsGroup>Barrel</linkedBodyPartsGroup>
      </li>
      <li Class="CombatExtended.ToolCE">
        <label>muzzle</label>
        <capacities>
          <li>Poke</li>
        </capacities>
        <power>8</power>
        <cooldownTime>1.55</cooldownTime>
        <armorPenetrationBlunt>2.755</armorPenetrationBlunt>
        <linkedBodyPartsGroup>Muzzle</linkedBodyPartsGroup>
      </li>
    </tools>
    <comps>
      <li Class="CombatExtended.CompProperties_AmmoUser">
        <magazineSize>30</magazineSize>
        <reloadTime>4</reloadTime>
        <ammoSet>AmmoSet_9x19mmPara</ammoSet>
      </li>
      <li Class="CombatExtended.CompProperties_FireModes">
        <aiUseBurstMode>FALSE</aiUseBurstMode>
        <aiAimMode>Snapshot</aiAimMode>
        <aimedBurstShotCount>3</aimedBurstShotCount>
      </li>
    </comps>
    <modExtensions>
      <li Class="MuzzleFlash.MuzzleFlashProps" MayRequire="IssacZhuang.MuzzleFlash">
        <def>MF_StandardMuzzleFalshTwo</def>
        <drawSize>(0.55,0.55)</drawSize>
        <offsets>
          <li>(0.32,0)</li>
        </offsets>
      </li>
    </modExtensions>
  </ThingDef>
